<h1>Log In</h1>

<%= render partial: 'layouts/errors', locals: {object: @user} %>

<%= form_for @user, url: '/login'  do |f| %>
  
  <%= f.label :user_name %>:
  <%= f.text_field :user_name %>
  <br><br>
  <%= f.label :password %>:
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit "Log In" %>

<% end %>